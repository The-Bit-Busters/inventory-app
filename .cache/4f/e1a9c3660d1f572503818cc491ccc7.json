nerated":{"line":1,"column":1856}}],"sources":{"react/components/AddModal.js":"import React from \"react\";\nimport '../stylesheets/productpage.css';\n\nfunction AddModal({ \n    addItems,\n    onClose,\n    setTitle,\n    setDescription,\n    setPrice,\n    title,\n    description,\n    price,\n    setCategory,\n    category,\n    setImage,\n    image, }) {\n\n  return (\n    <div className=\"modal\">\n      <div className=\"modal-content\">\n      <form className=\"updateForm\" onSubmit={(event) => {\n            event.preventDefault();  \n            addItems(); \n        }}>\n          <label className=\"updateLabel\">\n            Title:\n            <input\n              type=\"text\"\n              value={title}\n              onChange={(e) => setTitle(e.target.value)}\n            />\n          </label>\n          <label className=\"description-label\">\n            <p>Description:</p>\n            <textarea\n              value={description}\n              onChange={(e) => setDescription(e.target.value)}\n              style={{ color: \"black\" }}\n            />\n          </label>\n          <label>\n            Price:\n            <input\n              type=\"number\"\n              value={price}\n              onChange={(e) => setPrice(e.target.value)}\n            />\n          </label>\n          <select\n            value={category}\n            name=\"cateory\"\n            id=\"category-dropdown\"\n            onChange={(e) => setCategory(e.target.value)}\n          >\n            <p>Category</p>\n            <option value=\"\">Select a Category</option>\n            <option value=\"mens\">Mens</option>\n            <option value=\"womens\">Womens</option>\n            <option value=\"shoes\">Shoes</option>\n          </select>\n          <label>\n            Image:\n            <input\n              type=\"text\"\n              value={image}\n              onChange={(e) => setImage(e.target.value)}\n            />\n          </label>\n\n          <button type=\"submit\">Add Item</button>\n        </form>\n        <button onClick={onClose}>Close</button>\n      </div>\n    </div>\n  );\n}\n\nexport default AddModal;\n"},"lineCount":null}},"error":null,"hash":"e2645e75f46daf0f58dec51cf15005be","cacheData":{"env":{}}}